*
	margin: 0px
	padding: 0px;
#head
	background #ccc
#texto
	font-family: 'Cinzel'!important;
#quimera
	width: 55px!important
	height: 55px!important
#quimera-nav
	height: 78px
#quimera-img 
	width: 500px
	margin-top: -100px
	-webkit-filter: grayscale(100%);
#title
	font-weight: bold;
	font-size: 75px;
	font-family: 'Cinzel', serif;
.servicios
	margin-right 60px
.servicios-img
	width 160px
.tramita
	font-size 30px
	text-align center
	color: #fff
.ayuda,.preguntas
	color: #fff
.texto-grande
	font-size 20px
	color: #fff;
.preguntas > li >p:hover
	color #fff!important
	font-size 20px;
.preguntas > li >p:hover
	transition all .2s ease-in
